{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "plugin:react-hooks/recommended",
    "prettier"
  ],
  "parser": "./node_modules/@babel/eslint-parser/lib/index.js",
  "parserOptions": {
    "ecmaFeatures": {
      "globalReturn": false,
      "jsx": true
    },
    "babelOptions": {
      "configFile": "./babel.config.json"
    },
    "ecmaVersion": 12,
    "sourceType": "module",
    "allowImportExportEverywhere": false
  },
  "plugins": ["react"],
  "rules": {}
}
